{
  "name": "dcodegroup/laravel-xero-leave",
  "description": "This package provides the standard Xero functionality for submitting leave applications to Xero",
  "keywords": [
    "laravel",
    "xero",
    "dcode",
    "leave",
    "sync"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Nathan Rzepecki",
      "email": "nathan@lionslair.net.au",
      "homepage": "https://nathanrzepecki.me",
      "role": "Developer"
    },
    {
      "name": "Dcode Group",
      "email": "forge@dcodegroup.com",
      "homepage": "https://dcodegroup.com"
    }
  ],
  "require": {
    "php": "^7.4||^8.0",
    "laravel/framework": "^7.0|^8.0|^9.0|^10.0",
    "dcodegroup/laravel-configuration": "^0.2",
    "dcodegroup/laravel-xero-employee": "^0.1"
  },
  "require-dev": {
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "Dcodegroup\\LaravelXeroLeave\\": "src"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dcodegroup\\LaravelXeroLeave\\LaravelXeroLeaveServiceProvider"
      ]
    }
  }
}
